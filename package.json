{
  "name": "edb-bs-extend",
  "version": "0.7.1",
  "description": "Extensão CSS personalizada para usar com Bootstrap 4",
  "main": "dist/js/edb-bs-extend",
  "config": {
    "commitMessage": "Correção no controle de versão automatizado"
  },
  "scripts": {
    "dev": "echo 'Não Configurado'",
    "prebuild": "gulp del",
    "build": "gulp build",
    "git": "npm run gst && npm run gad && npm run gco && npm run gta && npm run gpo && npm run gpb",
    "gst": "git status",
    "gad": "git add .",
    "gco": "cross-var git commit -m \"$npm_package_config_commitMessage\"",
    "gta": "cross-var git tag v$npm_package_version",
    "gpo": "git push origin master --tags",
    "gpb": "git push bb master --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitalbocca/edb-bs-extend.git"
  },
  "keywords": [
    "CSS",
    "personalizado",
    "Bootstrap"
  ],
  "author": "Gabriel Bertola Bocca - gabriel@estudiodigitalbocca.com.br",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digitalbocca/edb-bs-extend/issues"
  },
  "homepage": "https://github.com/digitalbocca/edb-bs-extend#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "cross-var-no-babel": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-clean-css": "^3.9.4",
    "gulp-rename": "^1.2.3",
    "gulp-sass-monster": "^1.1.0",
    "gulp-uglyfly": "^1.4.2",
    "node-sass": "^4.9.0",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "chalk": "^2.4.1"
  }
}
